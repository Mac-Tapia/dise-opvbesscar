{
    "include": [
        "src/iquitos_citylearn",
        "scripts/run_oe*.py",
        "scripts/run_uncontrolled*.py",
        "scripts/run_oe2*.py",
        "scripts/run_sac_ppo_a2c_only.py",
        "scripts/run_sac_ppo_only.py",
        "scripts/save_ppo_result.py",
        "scripts/run_api.py"
    ],
    "exclude": [
        "**/__pycache__",
        ".venv",
        "data",
        "outputs",
        "checkpoints",
        "analyses",
        "reports",
        ".github",
        "scripts/run_a2c_only.py",
        "scripts/run_all_agents.py",
        "scripts/run_ppo_a2c_only.py",
        "scripts/run_ppo_clean.py",
        "scripts/run_ppo_fast.py",
        "scripts/run_ppo_only.py",
        "scripts/run_ppo_simulate_final.py",
        "scripts/run_sac_only.py",
        "scripts/run_training_sequence.py"
    ],
    "reportMissingImports": false,
    "reportMissingTypeStubs": false,
    "reportUntyped": false,
    "reportUntypedFunctionCalls": false,
    "reportUntypedBaseClass": false,
    "reportUntypedNamedTuple": false,
    "reportUntypedClassDecorator": false,
    "reportUntypedFunctionDecorator": false,
    "reportUntypedVariableDeclaration": false,
    "reportUnusedImport": "warning",
    "reportUnusedVariable": "warning",
    "reportGeneralTypeIssues": true,
    "reportOptionalMemberAccess": true,
    "reportArgumentType": true,
    "reportOperatorIssue": true,
    "reportIncompatibleVariableOverride": true,
    "reportIncompatibleMethodOverride": true,
    "reportAssignmentType": false,
    "reportConstantRedefinition": true,
    "reportUnknownParameterType": false,
    "reportUnknownMemberType": false,
    "reportUnknownVariableType": false,
    "reportUnknownArgumentType": false,
    "reportUnknownReturnType": false,
    "reportVariableAnnotationRequired": false,
    "pythonVersion": "3.11",
    "typeCheckingMode": "basic",
    "venvPath": ".venv",
    "stubPath": "."
}
